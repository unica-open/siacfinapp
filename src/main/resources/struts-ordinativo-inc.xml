			<result name="gotoGestioneOrdinativo"  type="redirectAction">gestioneOrdinativoIncassoStep1</result>
			<result name="labelSiope">/jsp/movgest/include/labelSiope.jsp</result>
		</action>
		
		<action name="nuovoAccertamentoOrdinativo" class="nuovoAccertamentoOrdinativoAction">
			<result name="success">/jsp/ordinativo/nuovoAccertamentoOrdinativo.jsp</result>
