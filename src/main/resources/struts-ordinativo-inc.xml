			<result name="gotoElencoOrdinativoIncasso_CollegaReversali" type="redirectAction">
				<param name="actionName">elencoOrdinativoIncassoCollegaReversali</param>
				<param name="uidOrdCollegaReversali">${uidOrdCollegaReversali}</param>
			</result>
			<result name="goToRicercaOrdinativoIncasso" type="redirectAction">ricercaOrdinativoIncasso</result>
			<result name="tendinoCausaliPerRicerca">/jsp/ordinativo/include/tendinoCausaliPerRicerca.jsp</result>
			<!-- <result name="refreshPopupModalImpegno">/jsp/liquidazione/include/modalImpegnoContent.jsp</result> -->	
		</action>
		
		<action name="elencoOrdinativoIncassoCollegaReversali" class="elencoOrdinativoIncassoCollegaReversaliAction">
			<result name="success">/jsp/ordinativo/elencoOrdinativoIncassoCollegaReversali.jsp</result>
			<result name="input">/jsp/ordinativo/elencoOrdinativoIncassoCollegaReversali.jsp</result>
			<result name="gotoErroreRicerca" type="redirectAction">
				<param name="actionName">ricercaOrdinativoIncasso</param>
				<param name="uidOrdCollegaReversali">${uidOrdCollegaReversali}</param>
			</result>
			<result name="elencoOrdinativoPagamento" type="redirectAction">elencoOrdinativoPagamento</result>
		</action>
		
		<action name="elencoOrdinativoIncasso" class="elencoOrdinativoIncassoAction">
			<result name="success">/jsp/ordinativo/elencoOrdinativoIncasso.jsp</result>
			<result name="input">/jsp/ordinativo/elencoOrdinativoIncasso.jsp</result>
			<!-- <result name="gotoErroreRicerca">/jsp/ordinativo/ricercaOrdinativoIncasso.jsp</result> -->
			<result name="gotoErroreRicerca" type="redirectAction">ricercaOrdinativoIncasso</result>
		</action>
		
		 
		<action name="gestioneOrdinativoIncassoStep2" class="gestioneOrdinativoIncassoStep2Action">
			<result name="success">/jsp/ordinativo/gestioneOrdinativoPagamentoStep2.jsp</result>
			<result name="input">/jsp/ordinativo/gestioneOrdinativoPagamentoStep2.jsp</result>
			<result name="aggiornaOrdinativo" type="redirectAction">
