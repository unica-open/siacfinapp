			<result name="dettaglioQuoteOrdinativoIncassoInc">/jsp/ordinativo/include/dettaglioQuoteOrdinativoCollegatoIncassoInc.jsp</result>
			<result name="dettaglioQuoteOrdinativoIncassoPag">/jsp/ordinativo/include/dettaglioQuoteOrdinativoCollegatoIncassoPag.jsp</result>
			<result name="gotoRipeti" type="redirectAction">
