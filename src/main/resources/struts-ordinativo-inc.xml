			<result name="vaiStep2" type="redirectAction">gestioneOrdinativoIncassoStep2</result>
			<result name="labelSiope">/jsp/movgest/include/labelSiope.jsp</result>		
		</action>

		<action name="annullaOrdinativoIncasso" class="annullaOrdinativoIncassoAction">
			<result name="success" type="redirectAction">
			  <param name="actionName">elencoOrdinativoIncasso</param>
			</result>  
			<result name="input">/jsp/ordinativo/elencoOrdinativoIncasso.jsp</result>
			<result name="gotoElencoOrdinativoIncasso" type="redirectAction">	
				<param name="actionName">elencoOrdinativoIncasso</param>
			</result>
	  	</action>

		
		<action name="gestioneOrdinativoIncassoStep3" class="gestioneOrdinativoIncassoStep3Action">
