			<result name="gotoConsulta" type="redirectAction">
				<param name="actionName">consultaOrdinativoIncasso</param>
				<param name="numero">${numeroOrdinativoStruts}</param>
				<param name="anno">${annoOrdinativoStruts}</param>
			</result>
